{
  "name": "artomweb.github.io",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:tailwind\" \"npm run start\"",
    "dev:tailwind": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "build": "npm run buildcss && webpack --mode production",
    "buildcss": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --minify",
    "start": "webpack serve --mode development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "flowbite": "^2.5.2",
    "tailwindcss": "^3.4.17",
    "webpack": "^5.97.1",
    "webpack-dev-server": "^5.2.0"
  },
  "devDependencies": {
    "daisyui": "^4.12.23",
    "webpack-cli": "^6.0.1"
  }
}
